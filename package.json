{
  "name": "uniswap-agent",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/app.ts",
    "build": "rm -rf dist && tsc",
    "start:dist": "node dist/src/app.js",
    "dev": "concurrently \"tsx src/app.ts\" \"make-agent dev -p 3000\"",
    "dev-testnet": "concurrently \"tsx src/app.ts\" \"make-agent dev -p 3000 -t\"",
    "serve": "node dist/api/index.js",
    "lint": "eslint '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "eslint --fix '{src,tests}/**/*.{js,jsx,ts,tsx}' && prettier --write '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --config jest.config.ts",
    "vercel-build": "echo hello",
    "build:deploy": "next build && make-agent deploy -u https://uniswap-agent-one.vercel.app/"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.1.10",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/smart-order-router": "^4.21.1",
    "cors": "^2.8.5",
    "ethers": "5.7.2",
    "express": "^4.21.2",
    "near-safe": "^0.9.14",
    "viem": "^2.28.0",
    "zerion-sdk": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "make-agent": "^0.2.12",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
